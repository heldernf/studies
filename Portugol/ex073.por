programa
{
	
	funcao inicio()
	{
		inteiro v[10], mult = 9
		para (inteiro cont = 0; cont < 10; cont++) {			
			v[cont] = mult
			mult --
			escreva("|" + cont + "|" + v[cont] + " ")
		}
	}
}
